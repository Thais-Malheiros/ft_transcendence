{
  "name": "ft_transcendence",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "tsc && npm run css",
    "css": "tailwindcss -i ./src/input.css -o ./dist/output.css --minify",
    "css:watch": "tailwindcss -i ./src/input.css -o ./dist/output.css --watch",
    "dev": "vite --host 0.0.0.0 --port $FRONT_PORT",
    "serve": "http-server . -p 8080",
    "start": "npm run build && npm run serve"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "undici-types": "^7.16.0",
    "vite": "^7.2.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DanielSurf10/ft_transcendence.git"
  },
